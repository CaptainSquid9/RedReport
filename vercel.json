{
  "version": 2,
  "builds": [
    {
      "src": "/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/server/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/src/index.html"
    }
  ]
}
